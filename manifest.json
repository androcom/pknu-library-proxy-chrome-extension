{
  "manifest_version": 3,
  "name": "PKNU Library Proxy Redirector",
  "version": "1.0",
  "description": "링크를 부경대학교 도서관 프록시 서버로 연결합니다.",
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["patterns.json"],
      "matches": ["<all_urls>"]
    }
  ]
}